###############
*~
\#*\#
/.emacs.desktop
/.emacs.desktop.lock
*.elc
auto-save-list
tramp

# Org-mode
.org-id-locations
*_archive

# flymake-mode
*_flymake.*
 
# eshell files
/eshell/history
/eshell/lastdir

# elpa packages
/elpa/

# reftex files
*.rel

# AUCTeX auto folder
/auto/

# cask packages
.cask/
dist/

# Flycheck
flycheck_*.el

# server auth directory
/server/

# projectiles files
.projectile

# Compiled source #
###################
*.com
*.class
*.dll
*.exe
*.o
*.so

# Packages #
############
# it's better to unpack these files and commit the raw source
# git has its own built in compression methods
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip

# Logs and databases #
######################
*.log
*.sql
*.sqlite

# Dropbox settings and caches
.dropbox
.dropbox.attr
.dropbox.cache

# Mac OS stuff #
################

*.DS_Store
*.DS_Store?
.AppleDouble
.LSOverride
# Icon must end with two \r
Icon

# Thumbnails
._*


